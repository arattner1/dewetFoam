FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (metal gas);

metal
{
    transportModel  Newtonian;
    nu              1e-06;
    rho             5000;
}

gas
{
    transportModel  Newtonian;
    nu              5e-05;
    rho             1;
}

sigma            0.2;

