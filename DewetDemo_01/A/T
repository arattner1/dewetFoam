FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    SideA
    {
        type            cyclic;
    }


    SideB
    {
        type            cyclic;
    }
	
    BottomWall
    {
		type           WallThermalResistance;
		R_wall         0.0001; //m^2-K/W
		T_inf          300; //K
        value          uniform 300;

    }

    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }

    FrontNBack
    {
        type            empty;
    }
}

// ************************************************************************* //
